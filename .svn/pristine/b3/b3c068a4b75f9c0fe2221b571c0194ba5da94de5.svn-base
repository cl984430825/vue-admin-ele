*{margin: 0;padding: 0;}
._left{
  margin-left: 20px;
}
._right{
  margin-right: 20px;
}
._top{
  margin-top: 20px;
}
._bottom{
  margin-bottom: 20px;
}
.appContainer{
  display: flex;
}
.app_head{
  width: 100%;
  height: 60px !important;
  box-shadow: 0 1px 4px rgba(0,21,41,.08);
}
.mainBox{
  box-sizing: border-box;
  overflow-y: scroll;
  height: calc(100vh - 60px - 40px);
  background: #f2f2f2;
}
.asideCls{
  height: 100vh;
  width: initial !important;
}
.blueText{
  color: #409EFF !important;
  cursor: pointer;
  white-space: nowrap;
}
.onUpload{
  transition: all .3s ease-in-out;
  width: 0;
  height: 2px;
  background-color: #1890FF;
  position: fixed;
  top: 0;
  z-index: 999;
}
.labelCls{
  font-size: 14px;
  color: #606266;
  white-space: nowrap;
}
.titleCls{
  font-size: 22px;
  color: #333333;
}
.flexBox{
  display: flex;
  align-items: center;
}
.flexBetween{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
/* element-ui样式修改 */
.el-menu-item-group__title{
  padding: 0 !important;
}
.el-submenu .el-menu-item{
  background-color: #1f2d3d !important;
}
.el-menu-vertical-demo:not(.el-menu--collapse) {
  width: 200px;
  min-height: 400px;
}
#login .el-input__inner{
  border: 0;
  background: none;
  font-size: 14px;
}
#login .el-checkbox__inner{
  background: none;
}
.el-tabs__item{
  padding-left: 30px !important;
  padding-right: 30px !important;
  font-size: 16px !important;
  height: 50px !important;
  line-height: 50px !important;
}
.el-tabs__active-bar{
  padding: 0 30px;
  margin-left: -30px;
}
